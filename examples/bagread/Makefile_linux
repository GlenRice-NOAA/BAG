dist: 
	@mkdir -p .obj/$(HOSTMACHINE)/bagread && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/$(HOSTMACHINE)/bagread/ && ( cd `dirname .obj/$(HOSTMACHINE)/bagread` && $(TAR) bagread.tar bagread && $(GZIP) bagread.tar ) && $(MOVE) `dirname .obj/$(HOSTMACHINE)/bagread`/bagread.tar.gz . && $(DEL_FILE) -r .obj/$(HOSTMACHINE)/bagread

