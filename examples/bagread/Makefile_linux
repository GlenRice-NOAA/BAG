all: Makefile_linux $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	test -d ../bin/$(HOSTMACHINE)/ || mkdir -p ../bin/$(HOSTMACHINE)/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile_linux: bagread.pro  $(QTDIR)/mkspecs/default/qmake.conf ../libincludes.pro \
		../appmaster.pro
	$(QMAKE) -o Makefile_linux bagread.pro
qmake: 
