../lib/$(HOSTMACHINE)/$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(OBJCOMP) 
	test -d ../lib/$(HOSTMACHINE)/ || mkdir -p ../lib/$(HOSTMACHINE)/
	-$(DEL_FILE) $(TARGET)
	$(AR) $(TARGET) $(OBJECTS) $(OBJMOC)
	-$(DEL_FILE) ../lib/$(HOSTMACHINE)/$(TARGET)
	-$(MOVE) $(TARGET) ../lib/$(HOSTMACHINE)/


mocables: $(SRCMOC)
